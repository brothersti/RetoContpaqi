@model Employee

<h2>@Model.Nombre</h2>

<div class="card">
    <div class="card-body">
        <h5 class="card-title">Descripción</h5>
        <p class="card-text">@Model.Nombre</p>
        <p class="card-text">@Model.ApellidoPaterno</p>
        <p class="card-text">@Model.ApellidoMaterno</p>
        <p class="card-text">@Model.Email</p>
        <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
</div>